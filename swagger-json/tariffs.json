                    }
                }
            }
        },
        "/tariffs/create.php": {
            "post": {
                "description": "Create tariff",
                "tags": [
                    "tariffs"
                ],
                "consumes": [
                    "multipart/form-data"
                ],
                "parameters": [
                    {
                        "name": "name",
                        "in": "formData",
                        "description": "Tariff name",
                        "required": true
                    },
                    {
                        "name": "duration",
                        "in": "formData",
                        "description": "Lesson duration",
                        "required": true
                    },
                    {
                        "name": "price",
                        "in": "formData",
                        "description": "Tariff price",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Tariff created",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "400": {
                        "description": "Schema error",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/tariffs/update.php": {
            "patch": {
                "description": "Update tariff data",
                "tags": [
                    "tariffs"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "data",
                        "in": "body",
                        "description": "Tariff data to update",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "example": "Имя тарифа"
                                },
                                "duration": {
                                    "type": "number",
                                    "example": "60"
                                },
                                "price": {
                                    "type": "number",
                                    "example": "4000"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Tariff updated",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "400": {
                        "description": "Schema error",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/tariffs/delete.php": {
            "delete": {
                "description": "Delete tariff",
