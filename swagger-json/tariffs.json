{
    "swagger": "2.0",
    "info": {
        "description": "",
        "version": "1.0.6",
        "title": "Selfree CRM API"
    },
    "servers": [
        {
            "url": "https://api.selfree-school.ru/"
        }
    ],
    "tags": [
        {
            "name": "tariffs",
            "description": "Тарифы"
        },
        {
            "name": "abonements",
            "description": "Абонементы"
        }
    ],
    "schemes": [
        "https",
        "http"
    ],
    "paths": {
        "/tariffs/read.php": {
            "get": {
                "description": "View tariff",
                "tags": [
                    "tariffs"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "Tariff ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Tariff",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/tariffs/list.php": {
            "get": {
                "description": "Tariff list",
                "tags": [
                    "tariffs"
                ],
                "responses": {
                    "200": {
                        "description": "Tariff list",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "content": {
                            "application/json": {}
